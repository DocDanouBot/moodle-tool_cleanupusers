<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="tool/deprovisionuser/db" VERSION="20120122" COMMENT="XMLDB file for Moodle tool/deprovisionuser"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="tool_deprovisionuser" COMMENT="deprovision user">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="archived" TYPE="int" LENGTH="1" NOTNULL="true"  DEFAULT="0" SEQUENCE="false"/>
                <!--datetime not supported by xmldb user Table uses also int-->
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="foreign" TYPE="foreign" FIELDS="id" REFTABLE="groups" REFFIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
